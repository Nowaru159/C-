#include <iostream>
# include <cstdlib>
using namespace std;
int main() {
  float *ptF = new float;
  int *ptl = new int(1989);
  int *pt=new int[3];
  *ptF=34.5;
  *(pt)=34; *(pt+1)=55; *(pt+2)=90;
  cout<< "\n\nConteudo de pt :" << pt;
  cout << "\tconteudo do endereço apontado : " << *pt;
  cout << "\n\nConteudo de ptl : "<<ptl;
  cout << "\t conteudo do endereço apontadado" << *ptl;
   cout << "\n\nConteudo de ptF : "<<ptF;
   cout << "\t conteudo do endereço apontadado" << *ptF;

  cout<< "\n\n Conteudo do vetor \n";
  cout << "\n"<<*pt<<"\t"<<*(pt+1)<<"\t"<<*(pt+2);
  delete ptF; delete ptl; delete pt;
  cout << "\n\n";}
